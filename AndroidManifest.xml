<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
	package="com.socialsquare.collectionmuseum" android:versionCode="1"
	android:versionName="1.0">
	<uses-sdk android:minSdkVersion="8" />

	<application android:icon="@drawable/icon" android:label="@string/app_name"
		android:debuggable="true">
		<activity android:name=".CollectionMuseumMain" android:label="@string/app_name"
			android:configChanges="orientation">
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />
				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>
		<activity android:name=".activities.CollectionActivity"
			android:label="@string/collection_titleNew" android:configChanges="orientation">
		</activity>
		<activity android:name=".activities.CollectionPopupActivity"
			android:configChanges="orientation"
			android:theme="@style/Theme.CustomDialog">
		</activity>
	</application>
</manifest>